package java0623;

import java.util.Scanner;

public class ex06 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("노동시간을 입력하세요 : ");
		int a =sc.nextInt();
		int money=(a>8?5000*8+(7500*(a-8)):5000*a);
		System.out.println("총 임금은 "+money+"원 입니다" );
	}
}
